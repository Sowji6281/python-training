class Solution(object):
    def rotate(self, nums, k):
        """
        :type nums: List[int]
        :type k: int
        :rtype: None Do not return anything, modify nums in-place instead.
        """
        k=k%len(nums)
        '''nums[:]=nums[-k:]+nums[:-k]'''
        def reve(1,r):
            while 1<r:
                nums[1],nums[r]=nums[r],num[1]
                1+=1
                r-=1
